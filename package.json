{
	"name": "code-clip-registry",
	"displayName": "Clipboard Registry",
	"description": "A Visual Studio Code plugin for a more advanced clipboard",
	"version": "0.0.1",
	"publisher": "SirTobi",
	"engines": {
		"vscode": "^0.10.1"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:clipreg.copyToRing",
		"onCommand:clipreg.pasteNextRingItem",
		"onCommand:clipreg.pasteRingItem"
    ],
    "main": "./out/src/main",
    "contributes": {
        "commands": [
            {
                "command": "clipreg.copyToRing",
                "title": "Copy to Ring"
            },
            {
                "command": "clipreg.pasteNextRingItem",
                "title": "Paste next Ring Item"
            },
            {
                "command": "clipreg.pasteRingItem",
                "title": "Paste Ring Item"
            }
        ]
	},
	"scripts": {
		"vscode:prepublish": "node ./node_modules/vscode/bin/compile",
		"compile": "node ./node_modules/vscode/bin/compile -watch -p ./"
	},
	"devDependencies": {
		"typescript": "^1.6.2",
		"vscode": "0.10.x"
	}
}